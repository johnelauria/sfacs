<h1>Listing teachers</h1>
<br>
<table class="bordered">
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Staffnum</th>
    <th>Fname</th>
    <th>Minitial</th>
    <th>Lname</th>
    <th>Suffix</th>
    <th>Address</th>
    <th>Telnum</th>
    <th>Cellnum</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teachers.each do |teacher| %>
  <tr>
    <td><%= teacher.username %></td>
    <td><%= teacher.email %></td>
    <td><%= teacher.staffnum %></td>
    <td><%= teacher.fname %></td>
    <td><%= teacher.minitial %></td>
    <td><%= teacher.lname %></td>
    <td><%= teacher.suffix %></td>
    <td><%= teacher.address %></td>
    <td><%= teacher.telnum %></td>
    <td><%= teacher.cellnum %></td>
    <td><%= link_to 'Show', teacher %></td>
    <td><%= link_to 'Edit', edit_teacher_path(teacher) %></td>
    <td><%= link_to 'Destroy', teacher, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

