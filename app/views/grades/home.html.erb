    <th>Grading System</th>
    <th>Class List </th>
   
  </tr>

<% @subjects.each do |subject| %>
  <tr>

    <td><%= subject.subjcode %></td>
    <td><%= subject.description %></td>
    <td><%= subject.block %></td>
    <td><%= subject.level %></td>
    <td><%= subject.days %></td>
    <td><%= subject.time1 %></td>
    <td><%= subject.time2 %></td>
     <td><%= form_tag("grades/gradingsystem", :method =>"post") do %>
