    <% end %></td>
    <td><%= form_tag("grades/viewlist", :method =>"post") do %>
	<%= hidden_field_tag(:dugas, subject.id)%>
	<%= submit_tag("View", :class => "button") %> 
	<% end %></td>
    </tr>
 
  <% end %>
   </table>